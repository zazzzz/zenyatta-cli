<div class="wrapper-header">
  <header>
    <div class="brand">
      <img src="<%= logo %>" alt="" class="logo">
      <span><%= title %></span>
    </div>

    <nav class="navigator">
      <ul class="menu">
        <% navigators.forEach(function(navi){ %>
          <li class="menu-item">
            <a href="<%= navi.link %>">
              <%= navi.title %>
            </a>
          </li>
          <% }); %>
      </ul>
    </nav>
  </header>

  <div class="header-contaier">
    <div class="left desc">
      <h1 class="brand-title"><%= title %></h1>
      <p class="brand-desc1"><%= subtitle %></p>
      <p class="brand-desc2"><%= description %></p>
      <p class="brand-desc3"><%= subdescription %></p>
    </div>
    <div class="right img">
      <img src="<%= cover %>" alt="">
    </div>
  </div>
</div>